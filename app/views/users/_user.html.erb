<tr class="<%= cycle('odd', 'even') %>">
<td><%=h user.login %></td>
<td><%=h user.email %></td>
<td><%=h user.activated_email %></td>
<td><%= if user.last_login; user.last_login.to_s(:short); end %></td>
<td><%= user.enabled ? 'Y' : 'N' %>
<% unless user == current_user %>
[<% if user.enabled %>
<%= link_to('disable', user_path(user.id), :method => :delete) %>
<% else %>
<%= link_to('enable', enable_user_path(user.id), :method => :put) %>
<% end %>]
<% end %>
</td>
<td>[<%= link_to 'edit', user_roles_path(user) %>]</td>
<td>[<%= link_to 'edit', edit_user_path(user) %>]</td>
</tr>
